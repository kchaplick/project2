  

<div class="container">
  <div class="row">
    <div class="col s8">
      <form class="breedSurvey">
        <div class="row">
          <div class="input-field col s6">
            <input placeholder="John Smith" id="name" type="text" class="validate">
            <label for="name">Name</label>
          </div>
          <div class="input-field col s6">
            <input id="zipcode" type="text" class="validate">
            <label for="zipcode">Zipcode</label>
          </div>
        </div>
        {{#each questionsAndChoices}}
        <div class="row">
          <div class="input-field col s12">
            <select id="{{this.id}}">
              <option value="" disabled selected>Choose your option</option>
              {{#each this.Choices}}
              <option value="{{this.value}}">{{this.choice}}</option>
              {{/each}}
            </select>
            <label class="question" data="{{this.question}}">{{this.question}}</label>
          </div>
        </div>
        {{/each}}

        <button id="submitSurvey" class="btn waves-effect waves-light" type="submit" name="action">
          Submit
        </button>
      </form>
    </div>
  </div>
</div>

<!--jquery-->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script>
