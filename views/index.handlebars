<div class="container">
  <div class ="row">
    <div class="col s8">
      <h1>{{question}}</h1>
      
      {{#each questionsAndChoices}}
      <div class="input-field col s12">
        <select>
        <option value="" disabled selected>Choose your option</option>
        {{#each this.choices}}
        <option value="{{this.id}}">{{this.choice}}</option>
        {{/each}}
        </select>
        <label class="question" data="{{this.id}}">{{this.question}}</label>
      </div>
      {{/each}}
    </div>
  </div>
</div>

{{!-- javascript tags --}}
 <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script>
<script src="/js/survey.js"></script>




